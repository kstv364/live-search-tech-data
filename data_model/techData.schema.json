{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://builtwith.com/techDataExample.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "D": "example.com",
            "SP": 0,
            "SD": "sub.example.com",
            "FI": "2018-09-24",
            "LI": "2019-09-03",
            "T": [
                {
                    "N": "Angular JS",
                    "FD": "2018-09-24",
                    "LD": "2019-06-03"
                },
                {
                    "N": "Content Delivery Network",
                    "FD": "2018-09-24",
                    "LD": "2019-06-03"
                },
                {
                    "N": "jQuery",
                    "FD": "2018-09-24",
                    "LD": "2019-04-17"
                },
                {
                    "N": "jQuery UI",
                    "FD": "2018-09-24",
                    "LD": "2019-04-17"
                },
                {
                    "N": "Font Awesome",
                    "FD": "2018-09-24",
                    "LD": "2018-10-04"
                },
                {
                    "N": "jQuery Marquee",
                    "FD": "2018-09-24",
                    "LD": "2019-04-17"
                },
                {
                    "N": "SignalR",
                    "FD": "2018-09-24",
                    "LD": "2019-04-17"
                },
                {
                    "N": "html5shiv",
                    "FD": "2018-09-24",
                    "LD": "2019-06-03"
                },
                {
                    "N": "CNZZ",
                    "FD": "2018-09-24",
                    "LD": "2019-04-17"
                },
                {
                    "N": "Provide Support",
                    "FD": "2018-09-24",
                    "LD": "2019-04-17"
                },
                {
                    "N": "Cloudflare DNS",
                    "FD": "2018-10-04",
                    "LD": "2019-07-11"
                },
                {
                    "N": "GoDaddy",
                    "FD": "2019-08-25",
                    "LD": "2019-08-25"
                },
                {
                    "N": "GoDaddy DNS",
                    "FD": "2019-08-25",
                    "LD": "2019-09-03"
                },
                {
                    "N": "New Relic",
                    "FD": "2019-09-03",
                    "LD": "2019-09-03"
                },
                {
                    "N": "Viewport Meta",
                    "FD": "2019-09-03",
                    "LD": "2019-09-03"
                },
                {
                    "N": "IPhone / Mobile Compatible",
                    "FD": "2019-09-03",
