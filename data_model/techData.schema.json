                },
                {
                    "N": "IIS 7",
                    "FD": "2019-09-03",
                    "LD": "2019-09-03"
                },
                {
                    "N": "ASP.NET",
                    "FD": "2019-09-03",
                    "LD": "2019-09-03"
                },
                {
                    "N": "ASP.NET 4.0",
                    "FD": "2019-09-03",
                    "LD": "2019-09-03"
                }
            ]
        }
    ],
    "required": [
        "D",
        "SP",
        "FI",
        "LI",
        "T"
    ],
    "properties": {
        "D": {
            "$id": "#/properties/D",
            "type": "string",
            "title": "Root Domain",
            "description": "The root domain (our key field)",
            "default": "",
            "examples": [
                "example.com"
            ]
        },
        "SP": {
            "$id": "#/properties/SP",
            "type": "integer",
            "title": "Technology Spend",
            "description": "How much a website is spending on web technology per month in USD",
            "default": 0,
            "examples": [
                0,12,324,3232
            ]
        },
        "SD": {
            "default": "",
            "description": "Subdomain or internal page where the technology was detected with /* meaning internal page",
            "examples": [
                "sub.example.com","example.com/*",null
            ],
            "title": "The SD schema"
        },
        "FI": {
            "$id": "#/properties/FI",
            "type": "string",
            "title": "First Indexed Date",
            "default": "",
            "examples": [
                "2018-09-24"
            ]
        },
        "LI": {
            "$id": "#/properties/LI",
            "type": "string",
            "title": "Last Indexed Date",
            "description": "The date the entire record was last updated.",
            "default": "",
            "examples": [
                "2019-09-03"
            ]
        },
        "T": {
            "$id": "#/properties/T",
            "type": "array",
            "title": "Technologies",
            "description": "The list of technologies that are found on this specific D/SD combination",
            "default": [],
            "examples": [
                [
                    {
                        "N": "Angular JS",
                        "FD": "2018-09-24",
                        "LD": "2019-06-03"
                    },
                    {
                        "N": "Content Delivery Network",
                        "FD": "2018-09-24",
                        "LD": "2019-06-03"
                    }
                ]
            ],
            "additionalItems": true,
            "items": {
                "$id": "#/properties/T/items",
                "anyOf": [
                    {
                        "$id": "#/properties/T/items/anyOf/0",
                        "type": "object",
                        "default": {},
                        "examples": [
                            {
                                "N": "Angular JS",
                                "FD": "2018-09-24",
                                "LD": "2019-06-03"
                            }
                        ],
                        "required": [
                            "N",
                            "FD",
                            "LD"
                        ],
                        "properties": {
                            "N": {
                                "$id": "#/properties/T/items/anyOf/0/properties/N",
                                "type": "string",
                                "title": "Technology Name",
                                "default": "",
                                "examples": [
                                    "Angular JS"
                                ]
                            },
                            "FD": {
                                "$id": "#/properties/T/items/anyOf/0/properties/FD",
                                "type": "string",
                                "title": "First Detected",
                                "description": "The date we first detected the technology",
                                "default": "",
                                "examples": [
                                    "2018-09-24"
                                ]
                            },
                            "LD": {
                                "$id": "#/properties/T/items/anyOf/0/properties/LD",
                                "type": "string",
                                "title": "Last Detected",
                                "description": "The date we last detected the technology",
                                "default": "",
                                "examples": [
                                    "2019-06-03"
                                ]
                            }
                        },
                        "additionalProperties": true
                    }
                ]
            }
        }
    },
    "additionalProperties": true
}